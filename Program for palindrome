#include<iostream>
 using namespace std;
int main()
{
    int n=111, a,b=0,num;
    num=n;
    while(n!=0)
    {
        a=n%10;
        b=a+(b*10);
        n=n/10;
    }
    
    if(num==b)
        cout << "The given number is palindrome" << endl;
    else
        cout << "The given number is not palindrome" << endl;
 
    return 0;
}
